<?xml version="1.0" encoding="UTF-8"?>
<data_type_mapping_set xmlns="http://www.wherescape.com/xml/red" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Hive (*) to Generic (*)" xsi:schemaLocation="http://www.wherescape.com/xml/red Data%20Type%20Mapping.xsd">
  <xml_version>2</xml_version>
  <mapping_set_from version="*">Hive</mapping_set_from>
  <mapping_set_to version="*">Generic</mapping_set_to>
  <mapping_set_description>-</mapping_set_description>
  <mapping_set_null_policy>AllowNulls</mapping_set_null_policy>
  <data_type_mapping match_data_type="char" output_data_type="char(%length)">
    <output_parameters>
      <output_basic_data_type>char</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="varchar" output_data_type="varchar(%length)">
    <output_parameters>
      <output_basic_data_type>varchar</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="tinyint" output_data_type="byteint">
    <output_parameters>
      <output_basic_data_type>byteint</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="smallint" output_data_type="smallint">
    <output_parameters>
      <output_basic_data_type>smallint</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="int" output_data_type="integer">
    <output_parameters>
      <output_basic_data_type>integer</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="integer" output_data_type="integer">
    <output_parameters>
      <output_basic_data_type>integer</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="bigint" output_data_type="bigint">
    <output_parameters>
      <output_basic_data_type>bigint</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="decimal" output_data_type="numeric(%precision)">
    <match_parameters>
      <match_precision>*</match_precision>
      <match_scale>0,NULL</match_scale>
    </match_parameters>
    <output_parameters>
      <output_basic_data_type>numeric</output_basic_data_type>
      <output_scale>NULL</output_scale>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="decimal" output_data_type="numeric(%precision,%scale)">
    <match_parameters>
      <match_precision>*</match_precision>
      <match_scale>1-*</match_scale>
    </match_parameters>
    <output_parameters>
      <output_basic_data_type>numeric</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="float" output_data_type="real">
    <output_parameters>
      <output_basic_data_type>real</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="double" output_data_type="double">
    <output_parameters>
      <output_basic_data_type>double</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="string" output_data_type="clob">
    <output_parameters>
      <output_basic_data_type>clob</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="boolean" output_data_type="boolean">
    <output_parameters>
      <output_basic_data_type>boolean</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="date" output_data_type="date">
    <match_parameters>
      <match_length>*</match_length>
      <match_scale>*</match_scale>
    </match_parameters>
    <output_parameters>
      <output_basic_data_type>date</output_basic_data_type>
      <output_length>NULL</output_length>
      <output_scale>NULL</output_scale>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="timestamp" output_data_type="timestamp(9)">
    <match_parameters>
      <match_length>*</match_length>
      <match_scale>*</match_scale>
    </match_parameters>
    <output_parameters>
      <output_basic_data_type>timestamp</output_basic_data_type>
      <output_length>NULL</output_length>
      <output_scale>9</output_scale>
    </output_parameters>
  </data_type_mapping>
</data_type_mapping_set>
