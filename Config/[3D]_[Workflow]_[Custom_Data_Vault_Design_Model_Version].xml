<?xml version="1.0" encoding="UTF-8"?>
<workflow xmlns="http://www.wherescape.com/xml/Workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wherescape.com/xml/Workflow workflow_4.xsd">
  <xml_version>4</xml_version>
  <name>Custom Data Vault Design Model Version</name>
  <type>Version</type>
  <button name="Manage Attribute Types">
    <button_icon>attribute_type_assign.png</button_icon>
    <button_type>WhereScape default</button_type>
    <button_value>Manage Attribute Types</button_value>
  </button>
  <button name="Create Table">
    <button_icon>table_create.png</button_icon>
    <button_type>WhereScape default</button_type>
    <button_value>Create Table</button_value>
  </button>
  <button name="Assign Attribute Types">
    <button_icon>attribute_type_assign.png</button_icon>
    <button_type>WhereScape default</button_type>
    <button_value>Assign Attribute Types</button_value>
  </button>
  <button name="Add Source Mapping">
    <button_icon>add_source.png</button_icon>
    <button_type>WhereScape default</button_type>
    <button_value>Add Source Mappings</button_value>
  </button>
  <button name="Compare Models">
    <button_icon>compare-models.png</button_icon>
    <button_type>WhereScape default</button_type>
    <button_value>Compare Models</button_value>
  </button>
  <button name="Merge Into Other Model">
    <button_icon>Mergemodels.png</button_icon>
    <button_type>WhereScape default</button_type>
    <button_value>Merge Into Other Model</button_value>
  </button>
  <button name="Generate Data Vault">
    <button_icon>cube_dim.png</button_icon>
    <button_type>WhereScape default</button_type>
    <button_value>Generate Data Vault</button_value>
  </button>
  <button name="Commit to GIT">
    <button_desc>Creates the XML file for the selected Model Version, then commits this file to GIT.</button_desc>
    <button_icon>[3D]_[Icon]_[git-commit]</button_icon>
    <button_type>Script</button_type>
    <button_value>ed3d_commit_to_git</button_value>
    <button_refresh>true</button_refresh>
  </button>
  <button name="Delete Model and Commit">
    <button_desc>Deletes model within the WS3D repository and the XML file for the selected Model Version, then commits this file to GIT.</button_desc>
    <button_icon>[3D]_[Icon]_[git-delete-commit]</button_icon>
    <button_type>Script</button_type>
    <button_value>ed3d_delete_model_commit_to_git</button_value>
    <button_refresh>true</button_refresh>
  </button>
  <button name="Import from GIT">
    <button_desc>Import Model Version from GIT, for the selected Model version</button_desc>
    <button_icon>[3D]_[Icon]_[git-compare]</button_icon>
    <button_type>Script</button_type>
    <button_value>ed3d_import_model_from_git</button_value>
    <button_refresh>true</button_refresh>
  </button>
</workflow>
