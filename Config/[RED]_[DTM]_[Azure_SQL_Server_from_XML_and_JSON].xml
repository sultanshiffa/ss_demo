<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<data_type_mapping_set name="Azure SQL Server from XML and JSON" xsi:schemaLocation="http://www.wherescape.com/xml/red ./Data%20Type%20Mapping.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.wherescape.com/xml/red">
	<mapping_set_description>Azure SQL DW from XML and JSON
</mapping_set_description>
	<mapping_set_null_policy>AllowNulls</mapping_set_null_policy>
	<data_type_mapping match_data_type="char" output_data_type="varchar(4000)" active_flag="true"></data_type_mapping>
	<data_type_mapping match_data_type="char" output_data_type="varchar(4000)" active_flag="true"></data_type_mapping>
	<data_type_mapping match_data_type="varchar" output_data_type="varchar(4000)" active_flag="true"></data_type_mapping>
	<data_type_mapping match_data_type="varchar" output_data_type="varchar(4000)" active_flag="true"></data_type_mapping>
	<data_type_mapping match_data_type="varchar" output_data_type="varchar(4000)" active_flag="true"></data_type_mapping>
	<data_type_mapping match_data_type="date" output_data_type="varchar(4000)" active_flag="true"></data_type_mapping>
	<data_type_mapping match_data_type="datetime" output_data_type="varchar(4000)" active_flag="true"></data_type_mapping>
	<data_type_mapping match_data_type="timestamp" output_data_type="varchar(4000)" active_flag="true"></data_type_mapping>
	<data_type_mapping match_data_type="boolean" output_data_type="varchar(4000)" active_flag="true"></data_type_mapping>
	<data_type_mapping match_data_type="string" output_data_type="varchar(64)" active_flag="true">
		<match_parameters>
			<match_length>1-63</match_length>
			<match_precision>*</match_precision>
			<match_scale>*</match_scale>
		</match_parameters>
	</data_type_mapping>
	<data_type_mapping match_data_type="string" output_data_type="varchar(%length)" active_flag="true">
		<match_parameters>
			<match_length>64-*</match_length>
			<match_precision>*</match_precision>
			<match_scale>*</match_scale>
		</match_parameters>
	</data_type_mapping>
	<data_type_mapping match_data_type="numeric" output_data_type="numeric(%precision)" active_flag="true">
		<match_parameters>
			<match_scale>0</match_scale>
		</match_parameters>
	</data_type_mapping>
	<data_type_mapping match_data_type="numeric" output_data_type="numeric(13)" active_flag="true">
		<match_parameters>
			<match_precision>*-12</match_precision>
			<match_scale>0</match_scale>
		</match_parameters>
	</data_type_mapping>
	<data_type_mapping match_data_type="numeric" output_data_type="numeric(13,%scale)" active_flag="true">
		<match_parameters>
			<match_precision>*-12</match_precision>
		</match_parameters>
	</data_type_mapping>
	<data_type_mapping match_data_type="numeric" output_data_type="numeric(%precision,%scale)" active_flag="true"></data_type_mapping>
</data_type_mapping_set>
