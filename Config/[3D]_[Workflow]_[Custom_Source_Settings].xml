<?xml version="1.0" encoding="UTF-8"?>
<workflow xmlns="http://www.wherescape.com/xml/Workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wherescape.com/xml/Workflow workflow_4.xsd">
  <xml_version>4</xml_version>
  <name>Custom Source Settings</name>
  <type>Category</type>
  <button name="Switch Branch">
    <button_desc>Change to a different Branch within the current GIT repository.</button_desc>
    <button_icon>copy-advanced.png</button_icon>
    <button_type>Script</button_type>
    <button_value>ed3d_switch_branch</button_value>
    <button_refresh>true</button_refresh>
  </button>
  <button name="Close Branch">
    <button_desc>Close the current Branch, with the option of merging into another Branch.</button_desc>
    <button_icon>discover.png</button_icon>
    <button_type>Script</button_type>
    <button_value>ed3d_close_branch</button_value>
    <button_refresh>true</button_refresh>
  </button>
  <button name="Create Branch">
    <button_desc>Create a new Branch within the current GIT repository</button_desc>
    <button_icon>create_overlay.png</button_icon>
    <button_type>Script</button_type>
    <button_value>ed3d_create_feature_branch</button_value>
    <button_refresh>true</button_refresh>
  </button>
  <button name="Pull">
    <button_desc>Pull from GIT Branch, this will refresh the design to current commit on the remote server</button_desc>
    <button_icon>[3D]_[Icon]_[git-pull-request]</button_icon>
    <button_type>Script</button_type>
    <button_value>ed3d_pull</button_value>
    <button_refresh>true</button_refresh>
  </button>
  <button name="Commit to GIT">
    <button_desc>Creates the XML file for the selected Model Version, then commits this file to GIT.</button_desc>
    <button_icon>[3D]_[Icon]_[git-commit]</button_icon>
    <button_type>Script</button_type>
    <button_value>ed3d_commit_config_to_git</button_value>
    <button_refresh>true</button_refresh>
  </button>
  <button name="Import from GIT">
    <button_desc>Import Model Version from GIT, for any Model Version found within this Category, found within the selected GIT branch.</button_desc>
    <button_icon>[3D]_[Icon]_[git-compare]</button_icon>
    <button_type>Script</button_type>
    <button_value>ed3d_import_model_from_git</button_value>
    <button_refresh>true</button_refresh>
  </button>
  <button name="Commit All models">
    <button_icon>[3D]_[Icon]_[git-commit]</button_icon>
    <button_type>Script</button_type>
    <button_value>ed3d_commit_all_models_to_git</button_value>
    <button_refresh>true</button_refresh>
  </button>
</workflow>
