<?xml version="1.0" encoding="UTF-8"?>
<data_type_mapping_set xmlns="http://www.wherescape.com/xml/red" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Redshift (*) to Generic (*)" xsi:schemaLocation="http://www.wherescape.com/xml/red Data%20Type%20Mapping.xsd">
  <xml_version>2</xml_version>
  <mapping_set_from version="*">Redshift</mapping_set_from>
  <mapping_set_to version="*">Generic</mapping_set_to>
  <mapping_set_description>-</mapping_set_description>
  <mapping_set_null_policy>AllowNulls</mapping_set_null_policy>
  <data_type_mapping match_data_type="int2" output_data_type="smallint">
    <output_parameters>
      <output_basic_data_type>smallint</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="int4" output_data_type="integer">
    <output_parameters>
      <output_basic_data_type>integer</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="int8" output_data_type="bigint">
    <output_parameters>
      <output_basic_data_type>bigint</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="numeric" output_data_type="numeric(%precision)">
    <match_parameters>
      <match_scale>0</match_scale>
    </match_parameters>
    <output_parameters>
      <output_basic_data_type>numeric</output_basic_data_type>
      <output_scale>NULL</output_scale>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="numeric" output_data_type="numeric(%precision,%scale)">
    <output_parameters>
      <output_basic_data_type>numeric</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="float4" output_data_type="real">
    <output_parameters>
      <output_basic_data_type>real</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="float8" output_data_type="double">
    <output_parameters>
      <output_basic_data_type>double</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="bool" output_data_type="boolean">
    <output_parameters>
      <output_basic_data_type>boolean</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="bpchar" output_data_type="nchar(%length)">
    <output_parameters>
      <output_basic_data_type>nchar</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="char" output_data_type="nchar(%length)">
    <output_parameters>
      <output_basic_data_type>nchar</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="varchar" output_data_type="nvarchar(%length)">
    <output_parameters>
      <output_basic_data_type>nvarchar</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="date" output_data_type="date">
    <output_parameters>
      <output_basic_data_type>date</output_basic_data_type>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="timestamp" output_data_type="timestamp(6)">
    <match_parameters>
      <match_scale>*</match_scale>
    </match_parameters>
    <output_parameters>
      <output_basic_data_type>timestamp</output_basic_data_type>
      <output_scale>6</output_scale>
    </output_parameters>
  </data_type_mapping>
  <data_type_mapping match_data_type="timestamptz" output_data_type="timestamp(6)">
    <match_parameters>
      <match_scale>*</match_scale>
    </match_parameters>
    <output_parameters>
      <output_basic_data_type>timestamp</output_basic_data_type>
      <output_scale>6</output_scale>
    </output_parameters>
  </data_type_mapping>
</data_type_mapping_set>
